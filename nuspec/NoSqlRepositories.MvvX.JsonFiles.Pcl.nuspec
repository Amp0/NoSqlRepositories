<?xml version="1.0" encoding="utf-8"?>
 <package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>NoSqlRepositories.MvvX.JsonFiles.Pcl</id>
        <version>{BuildNumberVersion}</version>
        <title>NoSqlRepository for MvvmCross using json files (MvvmCross plugin)</title>
        <authors>MACK Mathieu,alexandrebalan</authors>
        <owners>MACK Mathieu,alexandrebalan</owners>
        <licenseUrl>https://github.com/mathieumack/NoSqlRepositories</licenseUrl>
