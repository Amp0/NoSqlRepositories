<?xml version="1.0" encoding="utf-8"?>
 <package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>NoSqlRepositories.JsonFiles.Net</id>
        <version>{BuildNumberVersion}</version>
        <title>NoSqlRepository for .net applications using json files</title>
        <authors>MACK Mathieu,alexandrebalan</authors>
        <owners>MACK Mathieu,alexandrebalan</owners>
        <licenseUrl>https://github.com/mathieumack/NoSqlRepositories</licenseUrl>
        <projectUrl>https://github.com/mathieumack/NoSqlRepositories</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>This package contains a NoSqlRepository for .net apps using json files.</description>
        <tags>net json repository</tags>
        <dependencies>
          <group>
            <!-- Always include regardless of target framework -->
            <dependency id="NoSqlRepositories.Core" version="{BuildNumberVersion}" />
            <dependency id="Newtonsoft.Json" version="9.0.1" />
        </group>
        </dependencies>  
    </metadata>
    <files>     
        <!-- ALL -->
        <file src="..\NoSqlRepositories.JsonFiles.Net\bin\Release\NoSqlRepositories.JsonFiles.Net.XML" target="lib\net45" />
        <file src="..\NoSqlRepositories.JsonFiles.Net\bin\Release\NoSqlRepositories.JsonFiles.Net.dll" target="lib\net45" />
    </files>
</package>
