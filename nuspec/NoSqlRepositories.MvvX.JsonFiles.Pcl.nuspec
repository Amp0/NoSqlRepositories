        <file src="..\NoSqlRepositories.MvvX.JsonFiles.Pcl\bin\Release\NoSqlRepositories.MvvX.JsonFiles.Pcl.dll" target="lib\portable-net45+sl5+wp8+wpa81+win8+monoandroid+monotouch" />
